<?xml version='1.0' encoding='utf-8'?>
<scheduledTasks xmlns="http://xmlns.oracle.com/oim/scheduler">
    <task>
        <name>SuccessFactors Custom Picklist Lookups Reconciliation</name>
        <class>entel.oim.plugins.scheduler.CustomPicklistLookupsSuccessFactorsReconciliationTask</class>
        <description>Task that executes the custom picklist lookups from Success Factors</description>
        <retry>5</retry>
        <parameters>
            <string-param required="true" encrypted="false" helpText="IT Resource Name">IT Resource Name</string-param>
            <string-param required="true" encrypted="false" helpText="Picklist from Success Factors">Picklist</string-param>
            <string-param required="true" encrypted="false" helpText="Lookup Definition Code">Lookup Definition</string-param>
        </parameters>
    </task>
</scheduledTasks>
