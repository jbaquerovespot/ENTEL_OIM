<?xml version='1.0' encoding='utf-8'?>
<scheduledTasks xmlns="http://xmlns.oracle.com/oim/scheduler">
    <task>
        <name>SuccessFactors Update Pending User Login an Email accounts</name>
        <class>entel.oim.plugins.scheduler.UpdateUserEmailAccountSuccessFactorsTask</class>
        <description>Task that executes the update of attributes of users in Success Factors</description>
        <retry>5</retry>
        <parameters>
            <string-param required="true" encrypted="false" helpText="IT Resource Name">IT Resource Name</string-param>
            <string-param required="true" encrypted="false" helpText="Service from Success Factors">Service</string-param>
            <string-param required="true" encrypted="false" helpText="Contact Info Service from Success Factors">Contact Info Service</string-param>
            <string-param required="true" encrypted="false" helpText="Lookup Definition Code of Pending User Login">Lookup User Login</string-param>
            <string-param required="true" encrypted="false" helpText="Lookup Definition Code of Pending User Email">Lookup User Email</string-param>
        </parameters>
    </task>
</scheduledTasks>