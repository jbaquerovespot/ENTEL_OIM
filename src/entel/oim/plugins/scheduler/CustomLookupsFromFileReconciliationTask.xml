<?xml version='1.0' encoding='utf-8'?>
<scheduledTasks xmlns="http://xmlns.oracle.com/oim/scheduler">
    <task>
        <name>Custom Files Lookups Reconciliation</name>
        <class>entel.oim.plugins.scheduler.CustomLookupsFromFileReconciliationTask</class>
        <description>Task that executes the custom files lookups</description>
        <retry>5</retry>
        <parameters>
            <string-param required="true" encrypted="false" helpText="File to Read Lookups">File</string-param>
            <string-param required="true" encrypted="false" helpText="Lookup Definition Code">Lookup Definition</string-param>
        </parameters>
    </task>
</scheduledTasks>
